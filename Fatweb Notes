CUSTOM CSS


.x-logobar-inner {
  position:fixed;
  width:100%;
  background:#fff;
}
.x-navbar {
  top:112px
}
.admin-bar .x-navbar-fixed-top, .admin-bar .x-navbar-fixed-left, .admin-bar .x-navbar-fixed-right{
  top:142px;
}

.masthead-stacked .x-brand {
  float:left;
  width:20%;
  text-align:left;
}
.head-navbar-content {
  float:left;
  width:80%
}

@media (max-width:768px) {
  .masthead-stacked .x-brand {
    margin-bottom:10px;
  }
  .masthead-stacked .x-brand,
  .head-navbar-content {
    float:none;
    width:100%;
    text-align:center !important;
  }
  .x-logobar-inner {
    position:relative
  }
  .x-navbar {
    top:inherit
  }
}


